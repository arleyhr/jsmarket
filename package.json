{
  "name": "@jsmarket/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "commit": "cz",
    "prepare": "husky install",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "affected:build": "nx affected:build",
    "dev": "nx run-many -t serve -p api web --parallel",
    "dev:all": "nx run-many -t serve --all --parallel",
    "dev:api": "nx serve api",
    "dev:web": "nx serve web",
    "lint": "nx run-many -t lint -p api web --parallel",
    "test": "nx run-many -t test -p api web --parallel",
    "build:dev": "nx run-many -t build -p api web --configuration=development",
    "build:prod": "nx run-many -t build -p api web --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "graphql": "^16.9.0",
    "mysql2": "^3.11.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "xstate": "^5.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "20.0.7",
    "@nx/eslint-plugin": "20.0.7",
    "@nx/jest": "20.0.7",
    "@nx/js": "20.0.7",
    "@nx/nest": "^20.0.7",
    "@nx/node": "20.0.7",
    "@nx/react": "20.0.7",
    "@nx/vite": "20.0.7",
    "@nx/web": "20.0.7",
    "@nx/webpack": "20.0.7",
    "@nx/workspace": "20.0.7",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/react": "15.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.1",
