  "lint-staged": {
    "apps/**/*.{ts,js,tsx,jsx}": [
      "nx affected:lint --fix",
      "nx format:write"
    ],
    "libs/**/*.{ts,js,tsx,jsx}": [
      "nx affected:lint --fix",
      "nx format:write"
